<script type="text/javascript">
	lst = ['<span class="btn btn-lg btn-primary" onclick="clickfunc()">click count: ']
	cnt = 0;
	function clickfunc(){
		$("#main").html(lst[0] + cnt + '</span>');
		cnt = cnt + 1;
		
	}
</script>

<style type="text/css">
</style>

<div id="main">
	<span class="btn btn-lg btn-primary" onclick="clickfunc()">click me</span>
</div>
